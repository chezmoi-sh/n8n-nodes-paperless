{
	"name": "n8n-nodes-paperless",
	"version": "0.1.0",
	"description": "N8N community node manage your documents with Paperless-ngx.",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "",
	"author": {
		"name": "",
		"email": ""
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/xunleii/n8n-nodes-paperless-ngx.git"
	},
	"engines": {
		"node": ">=18.10",
		"pnpm": ">=9.1"
	},
	"packageManager": "pnpm@9.1.4",
	"main": "index.js",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"git:commit": "npx czg",
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/paperlessApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Paperless/Paperless.node.js"
		]
	},
	"devDependencies": {
		"@trunkio/launcher": "^1.3.4",
		"@types/node": "^18.10.0",
		"@typescript-eslint/parser": "^8.0.0",
		"eslint": "^8.57.1",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^4.0.2",
		"n8n-workflow": "^1.70.0",
		"prettier": "^3.4.2",
		"typescript": "^5.7.3"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"form-data": "^4.0.1"
	}
}
